# HomeFleet Environment Configuration
# Copy this file to .env and update with your actual values

# User and Group IDs
PUID=1000
PGID=1000

# Timezone
TZ=America/New_York

# Data directories - Update these paths for your system
DATADIR=/path/to/your/data/directory
CONFIG=/path/to/your/config/directory
DATA=/path/to/your/data
MOVIES=/path/to/your/movies
TV=/path/to/your/tv
TMPMEDIA=/path/to/your/temp/media

# Database passwords (replace with secure passwords)
DB_PASS=your_secure_db_password
MYSQL_ROOT=your_secure_mysql_root_password
MYSQL_PASS=your_secure_mysql_password
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_DB=postgres
POSTGRES_USER=postgres

# API keys and tokens (replace with actual values)
PUSHOVER_TOKEN=your_pushover_token
PUSHOVER_USER=your_pushover_user_key
PUSHOVER_WATCHTOWER=your_watchtower_pushover_token
ACME_EMAIL=your_email@example.com

# Traefik dashboard auth (replace with actual hash)
TRAEFIK_DASHBOARD_AUTH=admin:$2y$10$your_hashed_password_here

# Frigate configuration
FRIGATE_MEDIA=/path/to/frigate/media
FRIGATE_RTSP_PASSWORD=your_secure_frigate_password
ZWAVE_SESSION_SECRET=your_secure_zwave_session_secret

# Graylog configuration
GRAYLOG_PASSWORD_SECRET=your_secure_graylog_password_secret
GRAYLOG_ROOT_PASSWORD_SHA2=your_secure_graylog_root_password_hash
GRAYLOG_HTTP_EXTERNAL_URI=http://your-domain:9000/
